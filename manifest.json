{
  "manifest_version": 2,
  "name": "Playlist Length",
  "version": "1.0",
  "description": "Tells you the total length of a YouTube playlist.",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/playlist*"
      ],
      "js": [
        "src/duration.js",
        "src/yt-playlist-length.js"
      ]
    }
  ]
}
